<http://example.org/CCA/SolarEnergy>
    a 
        iop:Variable ;
    rdfs:label 
        "Solar energy production of building per month" ;
    skos:definition "Solar energy production of building in kWh";
    rdfs:comment "origin: photovoltaik panel, temporal extent: month, energy, amount, building" ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/131";
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/entity/Q40015> ;
    iop:hasMatrix 
        <https://www.wikidata.org/entity/Q41176> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q11379> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "origin: photovoltaik panel" ;
             iop:constrains <https://www.wikidata.org/entity/Q40015> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "temporal extent: month" ;
             iop:constrains <https://www.wikidata.org/entity/Q11379> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "type: solar" ;
             iop:constrains <https://www.wikidata.org/entity/Q40015> ;
        ] .

