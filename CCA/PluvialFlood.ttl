<http://example.org/iadopt/CCA/PluvialFlood>
    a 
        iop:Variable ;
    rdfs:label 
        "Pluvial flood due to 100 year event" ;
    rdfs:comment 
        """Pluvial flood due to 100 year event expressed as water depth caused by rainfall of 36 mm/h in a statistical unit, defined by a grouping of homogeneous neighboring building blocks""" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q283> ;
    iop:hasMatrix 
        <https://www.wikidata.org/wiki/Q3409269> ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q930412> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "defined by: grouping of homogeneous neighboring building blocks" ;
             iop:constrains <https://www.wikidata.org/wiki/Q3409269> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "due to: 36 mm/h rainfall" ;
             iop:constrains <https://www.wikidata.org/wiki/Q283> ;
        ] .

