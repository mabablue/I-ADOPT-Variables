<http://example.org/iadopt/CCA/FractionFloodedBuilding>
    a 
        iop:Variable ;
    rdfs:label 
        "Fraction of flooded area around buidling" ;
    rdfs:comment 
        """Fraction of the flooded area within a 5m buffer around a building which is flooded above a 30 cm water level""" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q11500> ;
    iop:hasMatrix 
        _:b0 ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q66055> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "spatial extent: buffer area of 5 m around an affected building" ;
             iop:constrains _:b0 ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "condition: flooded above 30 cm" ;
             iop:constrains <https://www.wikidata.org/wiki/Q11500> ;
        ] .

