@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <https://example.org/data/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix uom: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .

ex:State-of-tube
    a 
        iop:Variable ;
    rdfs:label 
        "State of connection pipe" ;
    skos:definition 
        """State (faulty, cracked, ... ) of pipe connection in plastic with a diametre of 30 cm.""" ;
    rdfs:comment
         "condition, pipe connection; plastic, diametre of 30 cm" ; 
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP60.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP12.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP61.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP62.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP64.yaml> ;
    iop:mapping <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/mapping/MR2.yaml> ;

    iop:hasObjectOfInterest 
        ex:ConnectingPipe ;
    iop:hasProperty 
        ex:StateOfPipeConnection ;
    iop:hasConstraint 
        [ a iop:Constraint, <https://www.wikidata.org/wiki/Q11474> ;
             rdfs:label "material type: plastic" ;
             iop:constrains ex:ConnectingPipe ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "size:diameter of 30 cm" ;
             iop:constrains ex:ConnectingPipe ;
        ] .

ex:PipeConnection
    a 
        iop:Entity ;
    rdfs:label 
        "pipe connection" .

<https://www.wikidata.org/wiki/Q813912>
    a 
        iop:Property ;
    rdfs:label 
        "state of pipe connection" .
