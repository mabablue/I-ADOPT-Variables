<http://example.org/iadopt/clinical/HRV>
    a 
        iop:Variable ;
    rdfs:label 
        "HRV Heart rate variabilty" ;
    skos:definition
          """HRV- Heart rate variability, Category: ECG metrics, Ontology: BMONT Unit: milliseconds (ms)""" ;      
    rdfs:comment 
        """contractions; person; ADD: role:patient; variation; time interval; type: heart; interval: n+(n+1)""" ;
    ex:issue
        "https://github.com/mabablue/I-ADOPT-examples-playground/issues/106" ;

    iop:hasObjectOfInterest 
        <https://www.wikidata.org/entity/Q127006> ;
    iop:hasMatrix 
        <https://www.wikidata.org/entity/Q5> ;
    iop:hasStatisticalModifier 
        <https://www.wikidata.org/entity/Q175199> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q186081> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "type: heart" ;
             iop:constrains <https://www.wikidata.org/entity/Q127006> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "interval: n+(n+1)" ;
             iop:constrains <https://www.wikidata.org/entity/Q127006> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "role: patient" ;
             iop:constrains <https://www.wikidata.org/entity/Q5> ;
        ] .

