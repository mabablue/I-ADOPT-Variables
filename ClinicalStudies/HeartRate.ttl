<http://example.org/iadopt/neurology/>
    a 
        iop:Variable ;
    rdfs:label 
        "BPM Mean heart rate" ;
    skos:definition 
        """BPM mean heart rate (beats per minute, ECG-derived)""" ;    
    rdfs:comment 
        """contractions; patient; mean; frequency; type: heart; ADD:patient""" ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/105" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/entity/Q127006> ;
    iop:hasMatrix 
        <https://www.wikidata.org/entity/Q181600> ;
    iop:hasStatisticalModifier 
        <https://www.wikidata.org/entity/Q2796622> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q11652> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "type: heart" ;
             iop:constrains <https://www.wikidata.org/entity/Q127006> ;
        ] .

