@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .


ex:C3
    a 
        iop:Variable ;
    rdfs:label 
        "Cloud cover" ;
    skos:definition 
        """The amount of sky estimated to be covered by all cloud types.""" ;
    rdfs:comment
        """sky; cloud; amount covered -> area fraction; ADD: area per area""" ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP10.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP21.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP26.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP71.yaml> ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/9" ;
    iop:hasObjectOfInterest 
        _:b0 ;
    iop:hasProperty 
        <http://www.ontology-of-units-of-measure.org/resource/om-2/AreaFraction> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "ratio: area per area" ;
             iop:constrains <http://www.ontology-of-units-of-measure.org/resource/om-2/AreaFraction> ;
        ] .

_:b0
    a 
        iop:Entity ,
        iop:AsymmetricSystem ;
    rdfs:label 
        "ratio" ;
    iop:hasNumerator 
        <https://www.wikidata.org/wiki/Q8074> ;
    iop:hasDenominator 
        <https://www.wikidata.org/wiki/Q527> .

<http://www.ontology-of-units-of-measure.org/resource/om-2/AreaFraction>
    a 
        iop:Property ;
    rdfs:label 
        "area fraction" .

<https://www.wikidata.org/wiki/Q8074>
    a 
        iop:Entity ;
    rdfs:label 
        "cloud" .

<https://www.wikidata.org/wiki/Q527>
    a 
        iop:Entity ;
    rdfs:label 
        "sky" .
