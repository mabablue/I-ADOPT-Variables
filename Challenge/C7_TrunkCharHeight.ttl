@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ex:C7 a iop:Variable ;
    rdfs:label "Maximum trunk char height" ;
    skos:definition """Maximum trunk char height, due to fire, measured above ground.""" ;
    rdfs:comment "trunk -> tree trunk, char; height, maximum, ADD: another component." ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP10.yam> ;  
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP17.yaml> ;
    ex:issue "https://github.com/i-adopt/examples/issues/24" ;
    iop:hasConstraint [ a iop:Constraint ;
            rdfs:label "aboveground" ;
            iop:constrains <http://qudt.org/vocab/quantitykind/Height> ] ;
    iop:hasMatrix <http://purl.jp/bio/4/id/200906090403689217> ;
    iop:hasObjectOfInterest <http://purl.jp/bio/4/id/200906050801342950> ;
    iop:hasProperty <http://qudt.org/vocab/quantitykind/Height> ;
    iop:hasStatisticalModifier <http://www.ontology-of-units-of-measure.org/resource/om-2/maximum>.

<http://purl.jp/bio/4/id/200906090403689217> a iop:Entity ;
    rdfs:label "tree trunk" .

<http://www.ontology-of-units-of-measure.org/resource/om-2/maximum> a iop:StatisticalModifier ;
    rdfs:label "maximum" .

<http://purl.jp/bio/4/id/200906050801342950> a iop:Entity ;
    rdfs:label "char" .

<http://qudt.org/vocab/quantitykind/Height> a iop:Property ;
    rdfs:label "height" .

