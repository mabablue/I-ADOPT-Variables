ex:C11
    a 
        iop:Variable ;
    rdfs:label 
        "Strike of bedding" ;
    skos:definition 
        """Geographic azimuth (relative to true north) at a point observation location, of a horizontal line contained in a sedimentary rock bedding surface. The bedding surface must not be horizontal; the azimuth is reported such that the dip direction of the inclined bedding is to the right when facing in the azimuth direction.""" ;
    rdfs:comment "line of intersection, sedimentary rock, azimuth, bedding plane, horizontal plane, ADD: interpretation: angle measured with the dip direction to the right";
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/17" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/entity/Q2234184> ;
    iop:hasMatrix 
        <https://www.wikidata.org/entity/Q82480> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q124274> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "interpretation: angle measured with the dip direction to the right when facing in the azimuth direction" ;
             iop:constrains <https://www.wikidata.org/entity/Q124274> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "type: strike of bedding" ;
             iop:constrains <https://www.wikidata.org/entity/Q124274> ;
        ] .

