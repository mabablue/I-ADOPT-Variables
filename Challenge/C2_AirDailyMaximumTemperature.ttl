ex:C2
    a 
        iop:Variable ;
    rdfs:label 
        "Air daily maximum temperature at 1.7m" ;
    skos:definition 
        """Thermodynamic temperature of the air in a height of 1.7 meter, daily maximum""" ;
    rdfs:comment
        """Thermodynamic temperature; air; in a height of 1.7 meter; daily; maximum - ADD: atmosphere (to distinguish it from indoors); 
VDP10 for property;  VDP40 for context object; VDP66 for constraint:datum; VDP67 for constraint:condition.""" ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP10.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP40.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP66.yaml> ;
    iop:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP67.yaml> ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q7391292> ;
    iop:hasContextObject 
        <https://www.wikidata.org/wiki/Q8104> ;
    iop:hasStatisticalModifier 
        <https://www.wikidata.org/wiki/Q10578722> ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q264647> ;
