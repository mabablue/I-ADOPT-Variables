ex:slvar
    a 
        iop:Variable ;
    rdfs:label 
        "solubility of naphthalene in water at room temperature" ;
    skos:definition 
        """solubility measurement of napathalene in deionized water at room temperature by monitoring absorption at 279 nm""" ;
    rdfs:comment 
        """solubility -> mass concentration; napathalene; deionized; water; at room temperature; absorption at 279 nm""" ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP17.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP34.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP70.yaml> ;
