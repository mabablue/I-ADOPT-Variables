    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP85.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP86.yaml> ;
    ex:issue <https://github.com/mabablue/I-ADOPT-examples-playground/issues/84> ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q179724> ;
    iop:hasMatrix 
        ex:Matrix ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q589446> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "phase: solid" ;
             iop:constrains <https://www.wikidata.org/wiki/Q179724> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "phase: liquid " ;
             iop:constrains <https://www.wikidata.org/wiki/Q283> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "requirement: local room temperature" ;
             iop:constrains <https://www.wikidata.org/wiki/Q589446> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "requirement: at 279 nm wavelength" ;
             iop:constrains <https://www.wikidata.org/wiki/Q589446> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "normalization: per unit volume" ;
             iop:constrains <https://www.wikidata.org/wiki/Q589446> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "condition: deionized" ;
             iop:constrains <https://www.wikidata.org/wiki/Q283> ;
        ] .

