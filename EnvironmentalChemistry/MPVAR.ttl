@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .

ex:mpvar
    a 
        iop:Variable ;
    rdfs:label 
        "melting point of naphthalene under standard conditions" ;
    skos:definition 
        """thermodynamic temperature for the melting point of a known purity (99.12% (w/w)) solid sample of naphthalene under standarding""" ;
    rdfs:comment 
        """thermodynamic temperature; melting point; purity (99.12% (w/w)); naphthalene;  under standarding -> at 1 atm pressure""" ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP70.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP84.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP85.yaml> ;
    ex:issue <https://github.com/mabablue/I-ADOPT-examples-playground/issues/79> ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q179724> ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q264647> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "condition: at the solid to liquid phase transition" ;
             iop:constrains <https://www.wikidata.org/wiki/Q264647> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "quality: purity of 99.12% (w/w)" ;
             iop:constrains <https://www.wikidata.org/wiki/Q179724> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "requirement: at 1 atm pressure" ;
             iop:constrains <https://www.wikidata.org/wiki/Q264647> ;
        ] .

