        <https://www.wikidata.org/entity/Q11465> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "datum: relative to instrument" ;
             iop:constrains <https://www.wikidata.org/entity/Q11465> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "location: at the surface" ;
