ex:RadVelDirHF
    a 
        iop:Variable ;
    rdfs:label 
        "DRVA Direction of radial velocity of water current relative to instrument and to True North by high frequency radar" ;
    skos:definition 
        """ Direction of radial velocity of water current relative to instrument and to True North by high frequency radar.""" ;
    rdfs:comment "Radial velocity (away from) of water current related to instrument; direction -> plane angle, relative to True North -> relative to True North" ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/62" ;
    iop:hasObjectOfInterest 
        <http://example.org/iadopt/RDVA_variable> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q2849635> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "datum: relative to True North" ;
             iop:constrains <https://www.wikidata.org/entity/Q2849635> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "sign meaning: positive degrees clockwise" ;
             iop:constrains <https://www.wikidata.org/entity/Q2849635> ;
        ] .

