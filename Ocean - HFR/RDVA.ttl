ex:RadVelErrHF
    a 
        iop:Variable ;
    rdfs:label 
        "RDVA Radial velocity (away from) of water current relative to instrument in the water body by High Frequency Radar" ;
    skos:definition 
        """Component of the sea surface current velocity that is directed toward (negative values) or away from (positive values) the High Frequency Radar (HFR) antenna.""" ;
    rdfs:comment "water current, water body, at the surface, sea, velocity, relative to instrument, positive away from the instrument, radial" ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/61" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/entity/Q5195029> ;
    iop:hasMatrix 
        <https://www.wikidata.org/entity/Q15324> ;
    iop:hasContextObject 
        <https://www.wikidata.org/entity/Q165> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q11465> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "datum: relative to instrument" ;
             iop:constrains <https://www.wikidata.org/entity/Q11465> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "location: at the surface" ;
             iop:constrains <https://www.wikidata.org/entity/Q15324> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "sign meaning: positive away from the instrument" ;
             iop:constrains <https://www.wikidata.org/entity/Q11465> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "type: radial" ;
             iop:constrains <https://www.wikidata.org/entity/Q11465> ;
        ] .

