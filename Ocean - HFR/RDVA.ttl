    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP68.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP81.yaml> ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q5195029> ;
    iop:hasMatrix 
        <https://www.wikidata.org/wiki/Q15324> ;
    iop:hasContextObject 
        <https://www.wikidata.org/wiki/Q165> ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q11465> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "relative to instrument" ;
             iop:constrains <https://www.wikidata.org/wiki/Q11465> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "at the surface" ;
             iop:constrains <https://www.wikidata.org/wiki/Q15324> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "positive away from the instrument" ;
             iop:constrains <https://www.wikidata.org/wiki/Q11465> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "radial" ;
             iop:constrains <https://www.wikidata.org/wiki/Q11465> ;
        ] .

