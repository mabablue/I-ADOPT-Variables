@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/HFR> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .

ex:Variable
    a 
        iop:Variable ;
    rdfs:label 
        "VART_QC Variance quality flag" ;
    skos:definition 
        """QC flags for radial velocity variance of water current, good data flag for those whose temporal variance is smaller than a maximum variance threshold.""" ;
    rdfs:comment "HCSS Radial velocity variance of water current, quality flag, good if under threshold value" ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP12.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP22.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP82.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP90.yaml> ;
    iop:hasObjectOfInterest 
        ex:OoI ;
    iop:hasProperty 
        ex:Prop ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "threshold: good if temporal variance is smaller than a maximum variance threshold" ;
             iop:constrains ex:Prop ;
        ] .

ex:OoI
    a 
        iop:Entity ;
    rdfs:label 
        "HCSS Radial velocity variance of water current" .

ex:Prop
    a 
        iop:Property ;
    rdfs:label 
        "quality flag" .
