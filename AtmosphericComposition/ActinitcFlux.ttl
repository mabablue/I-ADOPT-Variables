ex:ActinicFlux
    a 
        iop:Variable ;
    rdfs:label 
        "Upwelling actinic flux" ;
    rdfs:comment 
        """Upwelling component of actinic flux in wavelength range of 297-640 nm measured in photons/s/cm2/nm""" ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/103" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q7391292> ;
    iop:hasProperty 
        _:b0 ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "type: upwelling" ;
             iop:constrains _:b0 ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "condition: at 297-640 nm wavelength" ;
             iop:constrains _:b0 ;
        ] .

<https://www.wikidata.org/wiki/Q7391292>
    a 
