@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .

<http://example.org/iadopt/Amospheric/DeplRatio>
    a 
        iop:Variable ;
    rdfs:label 
        "Deplorizadion Ratio" ;
    rdfs:comment 
        """Ratio of backscattered light from the cross-polarized channel to the backscattered light from the parallet-polarized channel.""" ;
    iop:hasObjectOfInterest 
        ex:OoI ;
    iop:hasMatrix 
        _:b0 ;
    iop:hasProperty 
        ex:Prop ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "via: parallel-polarized channel" ;
             iop:constrains _:b1 ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "via: cross-polarized channel" ;
             iop:constrains _:b2 ;
        ] .

ex:OoI
    a 
        iop:Entity ;
    rdfs:label 
        "energy" .

_:b0
    a 
        iop:Entity ,
        iop:AsymmetricSystem ;
    iop:hasSource 
        _:b2 ;
    iop:hasTarget 
        _:b1 .

ex:Prop
    a 
        iop:Property ;
    rdfs:label 
        "ratio" .

_:b2
    a 
        iop:Entity ;
    rdfs:label 
        "backscattered light" .

_:b1
    a 
        iop:Entity ;
    rdfs:label 
        "backscattered light" .
