@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .
@prefix patt: <http://example.org/iadopt/pattern> .

ex:IceWater
    a 
        iop:Variable ;
    rdfs:label 
        "Ice water content in atmosphere" ;
    skos:definition 
        """mass concentration of ice in atmosphere measured in g/m3""" ;
    rdfs:comment 
        """g/m3-> mass concentration, ice -> water, state:solid, atmosphere""" ;
   ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/100" ;
    iop:hasObjectOfInterest 
        _:b0 ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q589446> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "state: solid" ;
             iop:constrains <https://www.wikidata.org/entity/Q283> ;
        ] .

_:b0
    a 
        iop:Entity ,
        iop:AsymmetricSystem ;
    iop:hasNumerator 
        <https://www.wikidata.org/entity/Q283> ;
    iop:hasDenominator 
        <https://www.wikidata.org/entity/Q8104> .

<https://www.wikidata.org/entity/Q589446>
    a 
        iop:Property ;
    rdfs:label 
        "mass concentration" .

<https://www.wikidata.org/entity/Q283>
    a 
        iop:Entity ;
    rdfs:label 
        "water" .

<https://www.wikidata.org/entity/Q8104>
    a 
        iop:Entity ;
    rdfs:label 
        "atmosphere" .
