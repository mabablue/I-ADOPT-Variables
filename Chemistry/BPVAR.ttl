@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .

ex:bpvar
    a 
        iop:Variable ;
    rdfs:label 
        "boiling point of benzene" ;
    skos:definition 
        """thermodynamic temperature for the transformation of a known purity (99.07% (v/v)) liquid sample of benzene into a gas (boiling point) at 1 atm pressure""" ;
    rdfs:comment 
        """thermodynamic temperature; at the boiling point -> liquid to gas phase transition; purity (99.07% (v/v)); benzene; at 1 atm pressure""" ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP10.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP70.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP75.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP84.yaml> ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/78" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q2270> ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q264647> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "temporal extent: liquid to gas phase transition" ;
             iop:constrains <https://www.wikidata.org/wiki/Q264647> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "condition: at 1 atm pressure" ;
             iop:constrains <https://www.wikidata.org/wiki/Q264647> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "quality: purity 99.07% (v/v)" ;
             iop:constrains <https://www.wikidata.org/wiki/Q2270> ;
        ] .

